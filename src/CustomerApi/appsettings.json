{
    "ConnectionStrings": {
        "Database": ""
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Error"
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "../../logs/log.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "AllowedHosts": "*"
}
